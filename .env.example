# GDG Community Companion Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local or any file with real API keys

# Google Cloud Project
GOOGLE_CLOUD_PROJECT=your-project-id-here

# Firebase Configuration (Frontend)
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789012
REACT_APP_FIREBASE_APP_ID=1:123456789012:web:abcdef123456
REACT_APP_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Pinecone Vector Database
PINECONE_API_KEY=pcsk_your-pinecone-api-key-here
PINECONE_HOST=https://your-index.svc.your-env.pinecone.io
PINECONE_PROJECT_ID=your-pinecone-project-id
PINECONE_INDEX_NAME=gdg-community
PINECONE_DIMENSION=1024
PINECONE_ENVIRONMENT=your-env

# LLM API Keys
GEMINI_API=your-gemini-api-key-here
OPENAI_API_KEY=sk-proj-your-openai-key-here

# LinkedIn Integration
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
LINKEDIN_ACCESS_TOKEN=your-linkedin-access-token
# LINKEDIN_ORGANIZATION_ID=<numeric-org-id-from-company-page>

# Bluesky Integration (optional)
# BLUESKY_IDENTIFIER=your-email@example.com
# BLUESKY_APP_PASSWORD=your-bluesky-app-password

# Firebase Admin SDK (if using service account)
# GOOGLE_APPLICATION_CREDENTIALS=path/to/serviceAccountKey.json

# Development Settings
REACT_APP_USE_EMULATORS=false
REACT_APP_MOCK_AUTH_ENABLED=false