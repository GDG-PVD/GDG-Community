# Production Environment Variables Example
# Copy to .env.production in your PRIVATE repository
# Never commit the actual .env.production file!

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id

# Feature Flags (Production settings)
REACT_APP_MOCK_AUTH_ENABLED=false
REACT_APP_USE_EMULATORS=false

# API Configuration
REACT_APP_API_BASE_URL=https://us-central1-your-project.cloudfunctions.net

# External Services
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_HOST=your-pinecone-host
PINECONE_INDEX_NAME=your-index-name

# Social Media
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
LINKEDIN_ACCESS_TOKEN=your-linkedin-access-token
LINKEDIN_ORGANIZATION_ID=your-org-id

# Bluesky (when ready)
BLUESKY_USERNAME=your-bluesky-username
BLUESKY_APP_PASSWORD=your-app-password

# Other Services
VERTEX_AI_PROJECT_ID=your-gcp-project
VERTEX_AI_LOCATION=us-central1