# Firebase Configuration - Example
# Copy to .env.production and replace with your actual values

# Get these values from Firebase Console > Project Settings
REACT_APP_FIREBASE_API_KEY=your-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=000000000000
REACT_APP_FIREBASE_APP_ID=1:000000000000:web:abcdef123456

# API Configuration
REACT_APP_API_BASE_URL=https://us-central1-your-project.cloudfunctions.net

# Feature Flags
REACT_APP_MOCK_AUTH_ENABLED=false
REACT_APP_USE_EMULATORS=false